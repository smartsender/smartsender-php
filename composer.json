{
  "name": "smartsender/smartsender-php",
  "description": "PHP client for SmartSender API v3",
  "type": "library",
  "version": "0.1",
  "homepage": "http://github.com/smartsender/smartsender-php",
  "license": "MIT",
  "keywords": [
    "SmartSender",
    "Smartsender",
    "smartsender",
    "sndmart",
    "email",
    "sms",
    "send",
    "marketing"
  ],
  "authors": [
    {
      "name": "Kir Chernenko",
      "email": "kirill@smartsender.io"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "replace": {
    "smartsender/smartsender-php": "*"
  },
  "autoload": {
    "psr-4": {
      "SmartSender\\V3\\": "src/V3"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmartSender\\Tests\\": "tests/"
    }
  }
}